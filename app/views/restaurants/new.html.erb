<h1>Reservester#new</h1>

<%= form_for @restaurant do |r| %>
  <p>
    <%= r.label :name %><br>
    <%= r.text_field :name %>
  </p>

  <p>
    <%= r.label :description %><br>
    <%= r.text_field :description %>
  </p>

  <p>
    <%= r.label :address %><br>
    <%= r.text_field :address %>
  </p>

  <p>
    <%= r.label :phone %><br>
    <%= r.text_field :phone %>
  </p>

  <p>
    <%= r.submit %>
  </p>
<% end %>
